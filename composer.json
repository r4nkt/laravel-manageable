{
    "name": "r4nkt/laravel-manageable",
    "description": "Track users who manages models in your Laravel app.  (Forked from signifly/laravel-manageable.)",
    "homepage": "https://github.com/r4nkt/laravel-manageable",
    "license": "MIT",
    "authors": [
        {
            "name": "Morten Poul Jensen",
            "email": "mpj@signifly.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/database": "^9.0",
        "illuminate/support": "^9.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "orchestra/testbench": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "R4nkt\\Manageable\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "R4nkt\\Manageable\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "R4nkt\\Manageable\\ManageableServiceProvider"
            ]
        }
    }
}
